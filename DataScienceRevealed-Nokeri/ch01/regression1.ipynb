   "source": [
    "def adj_r2_score(X, y_true, y_pred):\n",
    "    r2 = r2_score(y_true, y_pred)\n",
    "    adj_r2 = 1 - (1 - r2_score(y_true, y_pred)) * (len(y_true) - 1) / (\n",
    "        len(y_true) - X.shape[1] - 1\n",
    "    )\n",
    "    return adj_r2"
   ]
